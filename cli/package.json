{
  "name": "@infinityloop.labs/cli",
  "version": "0.1.0",
  "private": true,
  "description": "Template TypeScript CLI library compiled for Node.js",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "infinity-cli": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c && tsc -p tsconfig.types.json && chmod +x dist/cli.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "watch": "tsc -p tsconfig.json --watch",
    "start": "npm run build && node dist/cli.js"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^22.13.10",
    "rollup": "^4.40.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.17"
  }
}
